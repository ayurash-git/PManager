<Application x:Class="PManager.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:l="clr-namespace:PManager.WPF"
             xmlns:vm="clr-namespace:PManager.WPF.ViewModels"
             xmlns:vmb="clr-namespace:PManager.WPF.ViewModels.Base"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <vmb:ViewModelLocator x:Key="Locator"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
